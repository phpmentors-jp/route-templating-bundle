{
    "name": "phpmentors/route-templating-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for route templating",
    "keywords": ["routing", "symfony"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/config": "~4.0|~5.0",
        "symfony/dependency-injection": "~4.0|~5.0",
        "symfony/framework-bundle": "~4.0|~5.0",
        "symfony/http-kernel": "~4.0|~5.0",
        "symfony/routing": "~4.0|~5.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\RouteTemplatingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPMentors\\RouteTemplatingBundle\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
